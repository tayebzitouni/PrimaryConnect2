{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Data Source=PrimaryConnect.db"
  },
  "FileUpload": {
    "UploadFolder": "uploads"
  },

  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 104857600
    }
  }

}
