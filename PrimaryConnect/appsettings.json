{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Data Source=PrimaryConnect.db"
  },
  "FileUpload": {
    "UploadFolder": "uploads"
  },

  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 104857600
    }
  },

  "Jwt": {
    "Key": "your_super_secret_key_here_12345",
    "Issuer": "yourapp.com",
    "Audience": "yourapp.com",
    "ExpireMinutes": 60
  }

}
